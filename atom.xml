    <category term="个性化设置" scheme="http://kunpw.cn/tags/%E4%B8%AA%E6%80%A7%E5%8C%96%E8%AE%BE%E7%BD%AE/"/>
    
    <category term="hexo" scheme="http://kunpw.cn/tags/hexo/"/>
    
    <category term="github" scheme="http://kunpw.cn/tags/github/"/>
    
  </entry>
  
